{
  "name": "@justkordy/avia-connector-sdk",
  "version": "1.0.0",
  "description": "Server-only TypeScript SDK that accepts WebSocket connections from AviaConnector clients.",
  "license": "MIT",
  "author": "JustKordy",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --sourcemap --clean",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "ws": "^8.18.0"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.10",
    "rimraf": "^6.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.5.0",
    "ws": "^8.18.0"
  }
}